      "source": [
        "league_df = pd.DataFrame(columns=['date','time','matchday','team','opponent','place','result','s_goals', 'c_goals','team_streak','opp_streak','team_last_five','opp_last_five'])\n",
        "\n",
        "i = 0\n",
        "for row in matches_df.values:\n",
        "    result_home_team = ('W' if row[4] > row[5] else 'L' if row[4] < row[5] else 'D')\n",
        "    result_away_team = ('W' if row[5] > row[4] else 'L' if row[5] < row[4] else 'D')\n",
        "    date = datetime.datetime.strptime(row[0], \"%Y-%m-%dT%H:%M:%SZ\")\n",
        "    league_df.loc[i] = [date.date(),date.hour,row[1],row[2],row[3],'H',result_home_team,row[4],row[5],None,None,None,None]\n",
        "    i+=1\n",
        "    league_df.loc[i] = [date.date(),date.hour,row[1],row[3],row[2],'A',result_away_team,row[5],row[4],None,None,None,None]\n",
        "    i+=1"
      ],
      "execution_count": null,
